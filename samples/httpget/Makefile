# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug_osx
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_osx)
  httpget_config = debug_osx
endif
ifeq ($(config),release_osx)
  httpget_config = release_osx
endif

PROJECTS := httpget

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

httpget:
ifneq (,$(httpget_config))
	@echo "==== Building httpget ($(httpget_config)) ===="
	@${MAKE} --no-print-directory -C . -f httpget.make config=$(httpget_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f httpget.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_osx"
	@echo "  release_osx"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   httpget"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"